{
    "version": 2,
    "builds": [
        {
            "src": "app.py",
            "use": "@vercel/python"
        },
        {
            "src": "*.html",
            "use": "@vercel/static"
        },
        {
            "src": "*.js",
            "use": "@vercel/static"
        },
        {
            "src": "*.css",
            "use": "@vercel/static"
        }
    ],
    "routes": [
        {
            "src": "/api/(.*)",
            "dest": "app.py",
            "methods": ["GET", "POST", "OPTIONS"],
            "headers": {
                "Access-Control-Allow-Origin": "*",
                "Access-Control-Allow-Headers": "Content-Type",
                "Access-Control-Allow-Methods": "GET,POST,OPTIONS"
            }
        },
        {
            "src": "/",
            "dest": "index.html"
        },
        {
            "src": "/(.+)",
            "dest": "/$1"
        }
    ],
    "env": {
        "PYTHONPATH": ".",
        "RAPIDAPI_KEY": "7481d3b186msh641c6fc52c76f5fp1f3e47jsn0b2ad8d0a3c7"
    }
}
