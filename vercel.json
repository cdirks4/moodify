{
    "version": 2,
    "builds": [
        {
            "src": "app.py",
            "use": "@vercel/python"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "app.py"
        }
    ],
    "env": {
        "PYTHONPATH": ".",
        "FFMPEG_BINARY": "/usr/bin/ffmpeg"
    },
    "build": {
        "env": {
            "PYTHONPATH": "."
        }
    }
}
